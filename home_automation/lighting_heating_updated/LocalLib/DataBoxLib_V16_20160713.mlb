  VAR_INPUT
    dataBoxAddress   : udint;  (*`{ENU} destination address (DataBox)  {CSY} adresa cíle (adresa v DataBoxu)*)
    length           : uint;  (*`{ENU} size of data block (number of bytes, max 10000)  {CSY} velikost pøenášeného bloku (poèet bytù, max 10000)*)
  END_VAR
  VAR_IN_OUT
    variable         : TPtrNaPole;  (*`{ENU} source address (variable) {CSY} adresa zdroje (promìnná)*)
  END_VAR
END_FUNCTION

