        <Object index="#x9000" name="r{RACK}_p{POSITION}_Table_UNI_{CHANNEL}" type="_TTS_UNIINIT" access="rw">
          <Comment>
            <label lang="en">Initialization table of UNI channel mode</label>
            <label lang="cz">Inicializacni tabulka UNI režimu kanálu</label>
          </Comment>                                
          <SubObjects>
            <SubObject name="code" access="const">
              <DefaultData>C5</DefaultData>
            </SubObject>  
            <SubObject name="mode" access="const">
              <DefaultData>02</DefaultData>
            </SubObject>      
            <SubObject name="chMod" access="ro">
              <DefaultData>05</DefaultData>
            </SubObject>
            <SubObject name="adr" access="ro">
              <DefaultData>00</DefaultData>
              <ValueDependency> 
                <DataOf>r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.NodeAddress.Address</DataOf>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="speed" access="ro">
              <DefaultData>0E</DefaultData>
              <ValueDependency> 
                <DataOf>r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.BaudRate</DataOf>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="tor" access="ro">
              <DefaultData>04</DefaultData>
              <ValueDependency> 
                <DataOf>r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.ReceiveIdleTime</DataOf>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="tot" access="ro">
              <DefaultData>04</DefaultData>
              <ValueDependency> 
                <DataOf>r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.TransmitIdleTime</DataOf>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="sysMap" access="ro">
              <DefaultValue>0</DefaultValue>               
              <ValueDependency>
                <When condition="&quot;{SYSTEM}&quot; == &quot;FOXTROT2&quot;">
                  <MacroValue>${CHANNEL_CODE}</MacroValue>
                </When>
              </ValueDependency>              
            </SubObject>
            <SubObject name="lendi" access="ro">
              <DefaultValue>254</DefaultValue>
              <ValueDependency> 
                <Formula>@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.ReceiveZoneSize + 4</Formula>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="lendo" access="ro">
              <DefaultValue>254</DefaultValue>
              <ValueDependency> 
                <Formula>@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.TransmitZoneSize + 4</Formula>                                                            
              </ValueDependency>
            </SubObject>
            <SubObject name="idati" access="ro">
              <MacroValue>__offset32(r{RACK}_p{POSITION}_UNI_{CHANNEL}_IN)</MacroValue>            
            </SubObject>
            <SubObject name="idato" access="ro">
              <MacroValue>__offset32(r{RACK}_p{POSITION}_UNI_{CHANNEL}_OUT)</MacroValue>
            </SubObject>
            <SubObject name="lnc" access="ro">
              <DefaultData>00</DefaultData>
              <ValueDependency>                 
                <Formula>@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.StopBits + @r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.DataFormat + (@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.Parity &lt;&lt; 3)</Formula>                                                                      
              </ValueDependency>
            </SubObject>
            <SubObject name="msc" access="ro">
              <DefaultData>80</DefaultData>
              <ValueDependency> 
                <Formula>@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.ModemControl.RTSControl + (@r{RACK}_p{POSITION}_Settings_UNI_{CHANNEL}.ModemControl.TurnOffReceiver &lt;&lt; 3)</Formula>                                                                          
              </ValueDependency>
            </SubObject>
          </SubObjects>          
        </Object>
          
