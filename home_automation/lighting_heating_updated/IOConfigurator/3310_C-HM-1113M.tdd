        <Object index="#x9000" name="r{RACK}_p{POSITION}_C_HM_1113M_init" type="TCIB_3310_init" access="rw">
          <Comment>
            <label lang="cz">Inicializacni tabulka modulu C-HM-1113M</label>
            <label lang="en">Initialization table of C-HM-1113M unit</label>
          </Comment>
          <SubObjects>
            <SubObject name="Header" access="rw">
              <!-- hlavicka ini. tabulky -->
              <SubObjects>
                <SubObject name="code" access="ro">
                  <!-- kod modulu -->
                  <DefaultValue>3310</DefaultValue>
                </SubObject>
                <SubObject name="mode" access="ro">
                  <DefaultValue>0</DefaultValue>
                </SubObject>
                <SubObject name="fadr" access="rw">
                  <!-- HW adresa modulu (dlouha adresa) -->
                  <MacroValue>${FADR}</MacroValue>
                </SubObject>
                <SubObject name="ladr" access="rw">
                  <!-- logicka adresa modulu (kratka adresa) -->
                  <MacroValue>{POSITION}</MacroValue>
                </SubObject>
                <SubObject name="ndev" access="ro">
                  <!-- pocet zarizeni modulu -->
                  <DefaultValue>5</DefaultValue>
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="Dev" access="rw">
              <!-- zarizeni modulu -->
              <SubObjects>
                <SubObject name="Dev[0]" access="rw">
                  <!-- zarizeni 1, vstupni, STAT -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>80</DefaultData>
                      <!-- defaultne povolit -->
                      <ValueDependency>
                        <When condition="@Devices.StateDevice == 0">
                          <Data>00</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>8</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[1]" access="rw">
                  <!-- zarizeni 2, vstupni, 3xAI -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>80</DefaultData>
                      <!-- defaultne povolit -->
                      <ValueDependency>
                        <When condition="@Devices.AnalogInputsDevice.Enabled == 0">
                          <Data>00</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>48</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[2]" access="rw">
                  <!-- zarizeni 3, vystupni, 2xAO -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <DefaultData>81</DefaultData>
                      <!-- defaultne povolit -->
                      <ValueDependency>
                        <When condition="@Devices.AnalogOutputsDevice.Enabled == 0">
                          <Data>00</Data>
                        </When>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                      <ValueDependency>
                        <Formula>(0x01 * (@Devices.AnalogOutputsDevice.AO1_HaltingState == 1)) + (0x02 * (@Devices.AnalogOutputsDevice.AO2_HaltingState == 1)) </Formula>
                        <!-- osetreni vystupu pri halt -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
