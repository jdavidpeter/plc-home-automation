                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                      <ValueDependency>
                        <Formula>(0x01 * (@Devices.BinaryOutputsDevice.DO1_3.HaltingState == 1)) + (0x02 * (@Devices.BinaryOutputsDevice.DO4_6.HaltingState == 1)) + (0x04 * (@Devices.BinaryOutputsDevice.DO7_8.HaltingState == 1)) + (0x08 * (@Devices.BinaryOutputsDevice.DO9_10.HaltingState == 1)) + (0x10 * (@Devices.BinaryOutputsDevice.DO11.HaltingState == 1))</Formula>
                        <!-- osetreni vystupu pri halt -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>11</DefaultValue>
                    </SubObject>
                  </SubObjects>
