<!-- Initialization table object -->
        <Object index="#x9000" name="r{RACK}_p{POSITION}_Table" type="_TTS_OS1401" access="rw">
          <Comment>
            <label lang="en">Initialization table of OS-1401 module</label>
            <label lang="cz">Inicializační tabulka modulu OS-1401</label>
          </Comment>
          <SubObjects>
            <SubObject name="Head" access="ro">
              <SubObjects>
                <SubObject name="ModulID" access="const">
                  <DefaultValue>1401</DefaultValue>
                </SubObject>
                <SubObject name="Stat0" access="ro">
                  <DefaultValue>00</DefaultValue>
                  <ValueDependency>
                    <When condition="@General.REM != 0">
                      <Data>02</Data>
                    </When>
                  </ValueDependency>
                </SubObject>
                <SubObject name="Stat1" access="const">
                  <DefaultValue>00</DefaultValue>
                </SubObject> 
              </SubObjects>
            </SubObject> 
            <SubObject name="EDO0" access="ro">
              <DefaultData>80</DefaultData>
              <ValueDependency>
                <When condition="@Binary_Outputs.EDO0 == 0">
                  <Data>00</Data>
                </When>
              </ValueDependency>
            </SubObject>
            <SubObject name="EDO1" access="ro">
              <DefaultData>80</DefaultData>
              <ValueDependency>
                <When condition="@Binary_Outputs.EDO1 == 0">
                  <Data>00</Data>
                </When>
              </ValueDependency>
            </SubObject>
          </SubObjects>
        </Object>

<!-- OS-1401 module declaration -->
        <Object index="#xA000" name="r{RACK}_p{POSITION}_ModulE1" type="TModulE1" access="rw"> <!-- if="_PLCTYPE_ &lt; 11" -->
          <Comment>
            <label lang="en">OS-1401 module declaration</label>
            <label lang="cz">Deklarace modulu OS-1401</label>
          </Comment>
          <Excluded>&quot;{SYSTEM}&quot; != &quot;FOXTROT&quot;</Excluded>
          <SubObjects>
            <SubObject name="version" access="ro">
              <DefaultValue>1</DefaultValue>
            </SubObject>
            <SubObject name="rack" access="ro">   
