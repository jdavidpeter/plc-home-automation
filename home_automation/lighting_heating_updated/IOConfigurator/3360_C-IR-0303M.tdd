        <!-- inicializacni tabulka modulu (objekty #x9000 ...) -->
        <Object index="#x9000" name="r{RACK}_p{POSITION}_C_IR_0303M_init" type="TCIB_3360_init" access="rw">
          <SubObjects>
            <SubObject name="Header" access="rw">
              <!-- hlavicka ini. tabulky -->
              <SubObjects>
                <SubObject name="code" access="ro">
                  <!-- kod modulu -->
                  <DefaultValue>3360</DefaultValue>
                </SubObject>
                <SubObject name="mode" access="ro">
                  <DefaultValue>0</DefaultValue>
                </SubObject>
                <SubObject name="fadr" access="rw">
                  <!-- HW adresa modulu (dlouha adresa) -->
                  <MacroValue>${FADR}</MacroValue>
                </SubObject>
                <SubObject name="ladr" access="rw">
                  <!-- logicka adresa modulu (kratka adresa) -->
                  <MacroValue>{POSITION}</MacroValue>
                </SubObject>
                <SubObject name="ndev" access="ro">
                  <!-- pocet zarizeni modulu -->
                  <DefaultValue>5</DefaultValue>
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="Dev" access="rw">
              <!-- zarizeni modulu -->
              <SubObjects>
                <SubObject name="Dev[0]" access="rw">
                  <!-- zarizeni 1, vstup/vystupni, 3 DI/3 DO -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <ValueDependency>
                        <When condition="@Devices.BinaryInputsOutputsDevice.Enabled == 1">
                          <Value>0x83</Value>
                        </When>
                        <!-- povoleni zarizeni -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <!-- osetreni vystupu pri halt -->
                      <ValueDependency>
                        <Formula>
                          (0x01 * (@Devices.BinaryInputsOutputsDevice.DO1.HaltingState == 1)) +
                          (0x02 * (@Devices.BinaryInputsOutputsDevice.DO2.HaltingState == 1)) + 
                          (0x04 * (@Devices.BinaryInputsOutputsDevice.DO3.HaltingState == 1))
                        </Formula>
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>3</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[1]" access="rw">
                  <!-- zarizeni 2, vstupni, STAT -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <ValueDependency>
                        <When condition="@Devices.StateDevice == 1">
                          <Value>0x80</Value>
                        </When>
                        <!-- povoleni zarizeni -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>8</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[2]" access="rw">
                  <!-- zarizeni 3, vstupni, AI1 -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <ValueDependency>
                        <When condition="@Devices.AnalogInput1Device.Enabled == 1">
                          <Value>0x80</Value>
                        </When>
                        <!-- povoleni zarizeni -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[3]" access="rw">
                  <!-- zarizeni 4, vstupni, AI2 -->
