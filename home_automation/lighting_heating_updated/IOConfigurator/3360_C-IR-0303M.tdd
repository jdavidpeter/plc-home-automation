                    <SubObject name="typdev" access="rw">
                      <ValueDependency>
                        <When condition="@Devices.AnalogInput2Device.Enabled == 1">
                          <Value>0x80</Value>
                        </When>
                        <!-- povoleni zarizeni -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
                <SubObject name="Dev[4]" access="rw">
                  <!-- zarizeni 5, vstupni, AI3 -->
                  <SubObjects>
                    <SubObject name="typdev" access="rw">
                      <ValueDependency>
                        <When condition="@Devices.AnalogInput3Device.Enabled == 1">
                          <Value>0x80</Value>
                        </When>
                        <!-- povoleni zarizeni -->
                      </ValueDependency>
                    </SubObject>
                    <SubObject name="bloko" access="rw">
                      <DefaultData>00</DefaultData>
                    </SubObject>
                    <SubObject name="leindev" access="const">
                      <!-- delka vstupu v bitech -->
                      <DefaultValue>16</DefaultValue>
                    </SubObject>
                    <SubObject name="leoutdev" access="const">
                      <!-- delka vystupu v bitech -->
                      <DefaultValue>0</DefaultValue>
                    </SubObject>
                  </SubObjects>
                </SubObject>
              </SubObjects>
            </SubObject>
            <SubObject name="ezsenb" access="rw">
              <!-- ezs povoleni -->
              <ValueDependency>
                <Formula>
                  (0x01 * (@Devices.BinaryInputsOutputsDevice.DI1_Type != 0)) + 
                  (0x02 * (@Devices.BinaryInputsOutputsDevice.DI2_Type != 0)) + 
                  (0x04 * (@Devices.BinaryInputsOutputsDevice.DI3_Type != 0))
                </Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ezstype" access="rw">
              <!-- ezs vyvazeni -->
              <ValueDependency>
                <Formula>
                  (0x01 * (@Devices.BinaryInputsOutputsDevice.DI1_Type == 2)) + 
                  (0x02 * (@Devices.BinaryInputsOutputsDevice.DI2_Type == 2)) + 
                  (0x04 * (@Devices.BinaryInputsOutputsDevice.DI3_Type == 2))
                </Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="manDis" access="rw">
              <!-- zakazani manualniho rezimu vystupu -->
              <ValueDependency>
                <Formula>
                  (0x01 * (@Devices.BinaryInputsOutputsDevice.DO1.ManualModeDisable == 1)) + 
                  (0x02 * (@Devices.BinaryInputsOutputsDevice.DO2.ManualModeDisable == 1)) + 
                  (0x04 * (@Devices.BinaryInputsOutputsDevice.DO3.ManualModeDisable == 1))
                </Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="tPress" access="rw">
              <!-- dlouhy stisk -->
              <ValueDependency>
                <Formula>@Devices.BinaryInputsOutputsDevice.LongTime * 100</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai1type" access="rw">
              <!-- typ ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput1Device.Type</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai2type" access="rw">
              <!-- typ ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput2Device.Type</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai3type" access="rw">
              <!-- typ ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput3Device.Type</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai1tau" access="rw">
              <!-- filtrace ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput1Device.Filter * 10</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai2tau" access="rw">
              <!-- filtrace ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput2Device.Filter * 10</Formula>
              </ValueDependency>
            </SubObject>
            <SubObject name="ai3tau" access="rw">
              <!-- filtrace ai -->
              <ValueDependency>
                <Formula>@Devices.AnalogInput3Device.Filter * 10</Formula>
              </ValueDependency>
            </SubObject>
          </SubObjects>
        </Object>

        <!-- i/o data modulu -->
        <!-- vstupni data modulu (objekty #x6000 ...) -->
        <!-- binarni vstupy -->
        <!-- stavovy byte stat-->
        <!-- analogovy vstup ai1 -->
        <!-- analogovy vstup ai2 -->
        <!-- analogovy vstup ai3 -->
        <!-- vystupni data modulu (objekty #x7000 ...) -->
        <!-- binarni vystupy -->
        <Object index="#x6000" name="r{RACK}_p{POSITION}_IN" type="TCIB_C_IR_0303M_IN">
          <SubObjects>
            <SubObject name="DI">
              <DisplayName>
                <label lang="cz">DI</label>
                <label lang="en">DI</label>
              </DisplayName>
              <Disabled>@Devices.BinaryInputsOutputsDevice.Enabled == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="STAT">
              <DisplayName>
                <label lang="cz">STAT</label>
                <label lang="en">STAT</label>
              </DisplayName>
              <Xml>
                <Editor>
                  <Expanded>0</Expanded>
                </Editor>
              </Xml>
              <!-- uzel dat nerozbalovat -->
              <Disabled>@Devices.StateDevice == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="AI1">
              <DisplayName>
                <label lang="cz">AI1</label>
                <label lang="en">AI1</label>
              </DisplayName>
              <Transformation>
                <Conversion condition="@Devices.AnalogInput1Device.Type == 0x2E">94</Conversion>
                <!-- OV100k   konvertovat neznamenkove v setinach,  UW100TF -->
                <Conversion condition="@Devices.AnalogInput1Device.Type == 0x82">84</Conversion>
                <!-- O-2V     konvertovat neznamenkove,             UWTF    -->
                <Conversion condition="@Devices.AnalogInput1Device.Type == 0xA1">84</Conversion>
                <!-- 16bcitac konvertovat neznamenkove,             UWTF    -->
                <Conversion>93</Conversion>
                <!-- vse ostatni konvertovat znamenkove v setinach, SW100TF -->
              </Transformation>
              <Disabled>@Devices.AnalogInput1Device.Enabled == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="AI2">
              <DisplayName>
                <label lang="cz">AI2</label>
                <label lang="en">AI2</label>
              </DisplayName>
              <Transformation>
                <Conversion condition="@Devices.AnalogInput2Device.Type == 0x2E">94</Conversion>
                <!-- OV100k   konvertovat neznamenkove v setinach,  UW100TF -->
                <Conversion condition="@Devices.AnalogInput2Device.Type == 0x82">84</Conversion>
                <!-- O-2V     konvertovat neznamenkove,             UWTF    -->
                <Conversion condition="@Devices.AnalogInput2Device.Type == 0xA1">84</Conversion>
                <!-- 16bcitac konvertovat neznamenkove,             UWTF    -->
                <Conversion>93</Conversion>
                <!-- vse ostatni konvertovat znamenkove v setinach, SW100TF -->
              </Transformation>
              <Disabled>@Devices.AnalogInput2Device.Enabled == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
            <SubObject name="AI3">
              <DisplayName>
                <label lang="cz">AI3</label>
                <label lang="en">AI3</label>
              </DisplayName>
              <Transformation>
                <Conversion condition="@Devices.AnalogInput3Device.Type == 0x2E">94</Conversion>
                <!-- OV100k   konvertovat neznamenkove v setinach,  UW100TF -->
                <Conversion condition="@Devices.AnalogInput3Device.Type == 0x82">84</Conversion>
                <!-- O-2V     konvertovat neznamenkove,             UWTF    -->
                <Conversion condition="@Devices.AnalogInput1Device.Type == 0xA1">84</Conversion>
                <!-- 16bcitac konvertovat neznamenkove,             UWTF    -->
                <Conversion>93</Conversion>
                <!-- vse ostatni konvertovat znamenkove v setinach, SW100TF -->
              </Transformation>
              <Disabled>@Devices.AnalogInput3Device.Enabled == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
          </SubObjects>
        </Object>

        <Object index="#x7000" name="r{RACK}_p{POSITION}_OUT" type="TCIB_C_IR_0303M_OUT">
          <SubObjects>
            <SubObject name="DOs">
              <DisplayName>
                <label lang="cz">DOs</label>
                <label lang="en">DOs</label>
              </DisplayName>
              <Disabled>@Devices.BinaryInputsOutputsDevice.Enabled == 0</Disabled>
              <!-- zakazani objektu -->
            </SubObject>
          </SubObjects>
        </Object>
        
        
